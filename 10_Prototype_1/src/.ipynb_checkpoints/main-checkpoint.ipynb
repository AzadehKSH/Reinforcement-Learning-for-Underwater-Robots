    "o3d_vis.create_window(window_name = \"Robot Map 3D\")\n",
    "\n",
    "def open3d_draw(rob_pos, env_map : EnvironmentMap, occ, depth):\n",
    "    o3d_occ = o3d.geometry.PointCloud()\n",
    "    o3d_occ.points = o3d.utility.Vector3dVector([(x,y,z) for (x,y,z) in occ])\n",
    "\n",
    "    o3d_depth = o3d.geometry.PointCloud()\n",
    "    o3d_depth.points = o3d.utility.Vector3dVector([(x,y,z) for (x,y,z) in depth])\n",
    "    \n",
    "    o3d_vis.add_geometry(o3d_occ)\n",
    "    o3d_vis.add_geometry(o3d_depth)\n",
    "    o3d_vis.poll_events()\n",
    "    o3d_vis.update_renderer()"
